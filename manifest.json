{
    "name": "decktag",
    "manifest_version": 2,
    "version": "0.1.0",
    "description": "keep hash tag in your tweet form of tweetdeck",
    "content_scripts": [
        {
            "matches": ["https://tweetdeck.twitter.com/"],
            "js": ["decktag.js"]
        }
    ],
    "permissions": ["storage"],
    "options_page": "options.html"
}
